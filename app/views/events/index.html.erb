
        <h1>Voilà la liste des événements de Strasbourg: </h1>
        <% @events.each do |event| %>

        <!-- Bootstrap table -->
        <div class="list-group">
          <a href="<%=event_path(event.id) %>" class="list-group-item list-group-item-action active">
          <div class="d-flex w-100 justify-content-between">
          <h5 class="mb-1"><%= event.title %></h5>
          <small>3 days ago</small>
          </div>
          <p class="mb-1"><%= event.description %></p>
          <small><%= event.admin.first_name%></small>
          </a>
        </div>
        <br>
        <!-- Fin Bootstrap table -->
        <% end %>
